{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "dom", "dom.iterable"],
    "esModuleInterop": true,
    "noImplicitAny": true, // any타입을 입력했을 때 에러띄워줌
    "strictNullChecks": true, // null,undefined 타입을 조작하면 에러
    // "baseUrl": "src/",
    "strict": true,
    "removeComments": true
  }
}
